int pins[]={2,3,4,5,6,7,8};
int row=7;
int Cl=5;
int tmp1=370;
int tmp2=1200;
void setup() {
int i;
for (i=0;i<7;i++){pinMode(pins[i],OUTPUT);}  

}
void show(byte* X){
  int i,j;
  for(i=Cl-1;i>=0;i--){
    for(j=0;j<row;j++){
      digitalWrite(pins[j],bitRead(X[i],j));
      }
    delayMicroseconds(tmp1);
     }
   for (i=0;i<7;i++){
     digitalWrite(pins[i],LOW);
     }
}

void loop() {
 
show(A);
delayMicroseconds(tmp2);
show(S);
delayMicroseconds(tmp2);
show(S);
delayMicroseconds(tmp2);
show(E);
delayMicroseconds(7*tmp2);

}
